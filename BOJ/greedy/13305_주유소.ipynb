    "#7:32 -> \n",
    "\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "n=int(input()) \n",
    "km=list(map(int,input().split()))\n",
    "price=list(map(int,input().split()))\n",
    "\n",
    "minPrice=price[0]\n",
    "total=0\n",
    "\n",
    "for i in range(n-1):\n",
    "    if minPrice>price[i]:\n",
    "        minPrice=price[i]\n",
    "\n",
    "    total+=(minPrice*km[i])\n",
    "print(total)\n",
    "\n"
   ]
  }
