   "source": [
    "#11047 동전0\n",
    "N, K = map(int, input().split())\n",
    "\n",
    "coins = []\n",
    "for _ in range(N):\n",
    "    coin = int(input())\n",
    "    coins.append(coin)\n",
    "coins = sorted(coins, reverse = True)\n",
    "\n",
    "\n",
    "coin_cnt = 0\n",
    "\n",
    "for i in coins:\n",
    "    coin_cnt += K//i\n",
    "    K = K%i\n",
    "\n",
    "print(coin_cnt)\n"
   ]
  }
