   "source": [
    "n = int(input())\n",
    "ans = 0\n",
    "while True:\n",
    "    if n%5 == 0:\n",
    "        ans += n//5\n",
    "        break\n",
    "    else:\n",
    "        n-=2\n",
    "        ans+=1\n",
    "\n",
    "    if n<0:\n",
    "        break\n",
    "\n",
    "if n<0:\n",
    "    print(-1)\n",
    "else:\n",
    "    print(ans)"
   ]
