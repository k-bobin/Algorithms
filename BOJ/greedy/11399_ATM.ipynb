    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "n=int(input()) \n",
    "mins = list(map(int,input().split()))\n",
    "mins.sort()\n",
    "\n",
    "for i in range(1,n):\n",
    "    mins[i] = mins[i-1]+ mins[i]\n",
    "\n",
    "print(sum(mins))"
