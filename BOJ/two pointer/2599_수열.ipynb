    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "result =[]\n",
    "\n",
    "n, k = map(int,input().split())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "for i in range(n - k + 1):\n",
    "    result.append(sum(a[i:i+k]))\n",
    "        \n",
    "print(max(result))"
   ]
