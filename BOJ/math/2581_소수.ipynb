   "source": [
    "M = int(input())\n",
    "N = int(input())\n",
    "\n",
    "# process\n",
    "decimal = []\n",
    "for i in range(M, N+1):\n",
    "\tfor j in range(2, i+1):\n",
    "\t\tif j == i:\n",
    "\t\t\tdecimal.append(i)\n",
    "\t\tif i % j == 0:\n",
    "\t\t\tbreak\n",
    "\n",
    "# output\n",
    "if not decimal:\n",
    "\tprint(-1)\n",
    "else:\n",
    "\tprint(sum(decimal))\n",
    "\tprint(decimal[0])"
   ]
