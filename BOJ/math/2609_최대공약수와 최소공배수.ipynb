   "source": [
    "def gcd(a,b):\n",
    "    while b > 0:\n",
    "        a,b = b, a%b\n",
    "    return a\n",
    "\n",
    "def lcm(a,b):\n",
    "    return a*b // gcd(a,b)\n",
    "\n",
    "n,m = map(int, input().split())\n",
    "\n",
    "print(gcd(n,m))\n",
    "print(lcm(n,m))"
   ]
  }
