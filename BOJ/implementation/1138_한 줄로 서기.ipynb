   "source": [
    "n = int(input())\n",
    "h = list(map(int, input().split()))\n",
    "ans = [0] * n\n",
    "for p in range(1, n+1):\n",
    "    t = h[p-1]\n",
    "    cnt = 0\n",
    "    for i in range(n):\n",
    "        if cnt == t and ans[i] == 0:\n",
    "            ans[i] = p\n",
    "            break\n",
    "        elif ans[i] == 0:\n",
    "            cnt += 1\n",
    "print(*ans)"
   ]
