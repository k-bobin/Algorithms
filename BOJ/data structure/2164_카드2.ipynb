   "source": [
    "from collections import deque\n",
    "n = int(input())\n",
    "_list= deque([i for i in range(1,n+1)])\n",
    "\n",
    "while len(_list) > 1:\n",
    "    _list.popleft()\n",
    "    next = _list.popleft()\n",
    "    _list.append(next)\n",
    "\n",
    "print(*_list)\n",
    "\n",
    "\n"
   ]
