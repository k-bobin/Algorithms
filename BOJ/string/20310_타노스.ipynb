    "def solution() :\n",
    "    n = list(input())\n",
    "    zero = n.count('0') // 2\n",
    "    one = n.count('1') // 2\n",
    "    \n",
    "    for _ in range(zero) :\n",
    "        n.pop(-(n[::-1].index('0'))-1)\n",
    "    \n",
    "    for _ in range(one) :\n",
    "        n.pop(n.index('1'))\n",
    "    \n",
    "    print(''.join(n))\n",
    "    \n",
    "solution()"
