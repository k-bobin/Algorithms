    "from itertools import permutations\n",
    "\n",
    "letters = list(input())\n",
    "n = len(letters)\n",
    "msg = \"I'm Sorry Hansoo\"\n",
    "\n",
    "for i in permutations(letters,n):\n",
    "    if list(i) ==list(reversed(i)):\n",
    "        msg = \"\".join(list(i))\n",
    "        break\n",
    "    \n",
    "print(msg)"
   ]
  }
